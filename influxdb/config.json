{
    "name": "InfluxDB",
    "version": "0.1",
    "slug": "influxdb",
    "description": "An timeseries database server",
    "url": "https://github.com/thomaspurchas/hassio-addons/tree/master/influxdb",
    "startup": "system",
    "boot": "auto",
    "ports": {
      "8086/tcp": 8086
    },
    "options": {
      "databases": ["homeassistant"]
    },
    "schema": {
      "databases": ["str"]
    },
    "image": "thomaspurchas/hassio-addon-influxdb-{arch}",
    "timeout": 20
  }
